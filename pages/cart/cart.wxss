.lp_cart_null{
width: 200px;
margin:0 auto;
margin-top: 100px;
}
.cart_item_icon{
 width: 100%;
 display: inline-flex;
 justify-content: center;
 align-items: center;
}
.lp_cart_null image{
  width:50%;
  height: 80px;
}
.lp_cart_null view{
  width: 100%;
  font-size: 14px;
  color:#999999;
   margin:10px auto;
   text-align: center;
}
.lp_cart_null button{
  width:130px;
  height:37px;
  color:white;
  background-color:#E64340;
  border-radius: 8px;
  line-height: 37px;
}
.cart_good_container{
  display: inline-flex;
  width: 100%;
}
.cate_item_icon_image{
  width: 20%;
  display: inline-flex;
  justify-content: center;
  align-items: center;
  height: 100px;
  margin-left: 5%;
}
.cate_item_icon_image image{
  width: 100%;
  height: 85px;
}
.cart_item_title{
  width: 55%;
  padding-left: 15px;
  padding-top: 15px;
}
.right_cart_container{
  width: 20%;
  padding-top: 15px;
}
.right_cart_container image{
  width: 25px;
  height: 25px;
  margin-top: 5px;
  margin-bottom: 25px;
}
.count_computed{
  margin-right:20px;
}
.count_computed button{
    border:1px solid #e6e6e6;
    width: 25px;
    height: 25px;
    display: inline-flex;
    align-items: center;
    justify-content: center;
}