/* pages/meDiscount/meDiscount.wxss */
.jbn_meDiscountList{
  width:100%;
  height:300rpx;
  border-top:1rpx solid #e4e4e4;
  display: flex;
  padding:38rpx 30rpx;
  box-sizing: border-box;
  justify-content: space-between;
  flex-wrap: wrap;
}
.jbn_meDiscountList view{
  width:216rpx;
  border:2rpx solid #77bb96;
  border-radius:5rpx;
  display: flex;
  flex-direction: column;
  font-size:26rpx;
  color:#84b190;
  text-align:center;
  margin-top:10rpx;
}
.jbn_meDiscountList view text{
  line-height:24rpx;
}
