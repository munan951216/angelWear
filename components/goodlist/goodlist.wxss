.good_container{
  display: inline-flex;
  width: 100%;
  justify-content: center;
  align-items: center;
  flex-wrap: wrap;
  margin-bottom: 15px;
}
.good_item{
  width: 20%;
  margin-top: 15px;
}
.good_item image{
  width:60%;
  height: 80rpx;
  margin-bottom:10px;
}
.icon_container{
  display: inline-flex;
  width: 100%;
  justify-content: center;
  align-items: center;
}
.good_item text{
  width: 100%;
  display: inline-flex;
  align-items: center;
  justify-content: center;
  font-size: 13px;
}